{
  "name": "tantor-learning",
  "version": "0.1.20",
  "description": "E-Learning Plateforme",
  "type": "commonjs",
  "author": "https://davmaene.github.io",
  "private": true,
  "license": "UNLICENSED",
  "overrides": {
    "jszip": "^3.10.1",
    "lodash.merge": "^4.6.2"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:memory": "node --max-old-space-size=4096 node_modules/@nestjs/cli/bin/nest.js start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node --max-old-space-size=4096 dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pre-commit": "node -e \"require('child_process').execSync('scripts/pre-commit-check.bat', {stdio: 'inherit'})\"",
    "security-audit": "npm audit --audit-level=moderate",
    "type-check": "tsc --noEmit",
    "monitor-memory": "node scripts/monitor-memory.js",
    "start:dev:monitor": "node --max-old-space-size=4096 --expose-gc node_modules/@nestjs/cli/bin/nest.js start --watch",
    "dev:hot": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/main.ts",
    "postinstall": "npm rebuild --arch=x64 --platform=linux sharp"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.0",
    "@nestjs/sequelize": "^11.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.0",
    "@types/nodemailer": "^6.4.17",
    "@types/randomstring": "^1.3.0",
    "base-64": "^1.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^1.41.3",
    "docx": "^9.4.1",
    "dotenv": "^16.4.7",
    "googleapis": "^148.0.0",
    "handlebars": "^4.7.8",
    "html-docx-js": "^0.3.1",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "mediasoup": "^3.19.2",
    "moment": "^2.30.1",
    "multer-storage-cloudinary": "^4.0.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.14.0",
    "nodejs-base64": "^2.0.0",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pdf-lib": "^1.17.1",
    "pg": "^8.14.1",
    "pg-hstore": "^2.3.4",
    "puppeteer": "^24.7.2",
    "randomstring": "^1.3.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^18.3.0",
    "typeorm": "^0.3.21"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.7",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
